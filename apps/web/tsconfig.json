{
    "compilerOptions": {
      "target": "es2020",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "module": "esnext",
      "moduleResolution": "node",
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "noUncheckedIndexedAccess": true,
      "baseUrl": ".",
      "paths": {
        "~/*": ["./src/*"]
      }
    },
    "include": [
      ".eslintrc.cjs",
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/*.cjs",
      "**/*.mjs"
    ],
    "exclude": ["node_modules"]
  }
  