{
  "name": "earthlings-must-die",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --force",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run hardhat:test --scope=chain",
    "compile": "turbo run hardhat:compile --scope=chain"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.10.7"
  },
  "packageManager": "npm@9.5.0",
  "dependencies": {
    "@paperxyz/js-client-sdk": "^0.2.5",
    "api": "^6.0.0"
  }
}
