{
  "name": "earthlings-must-die",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run hardhat:test --scope=chain",
    "compile": "turbo run hardhat:compile --scope=chain",
    "deploy": "turbo run hardhat:deploy --scope=chain"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "packageManager": "npm@9.5.0",
  "dependencies": {
    "@moralisweb3/common-evm-utils": "^2.22.2",
    "moralis": "^2.22.2"
  }
}
